{
  "name": "nextjs14-qrcode-generator",
  "version": "1.5.32",
  "private": true,
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "clean:files": "rm -rf node_modules && rm -rf .next && rm -f package-lock.json",
    "clean:cache": "npm cache clean --force && npm cache verify",
    "clean": "npm run clean:files && npm run clean:cache",
    "upgrade:latest": "npm run clean && npx npm-check-updates -u && npm install",
    "reinstall": "npm run clean && npm install",
    "logs:test": "npx tsx scripts/test-unified-logging.ts",
    "logs:cleanup": "npx prisma db execute --file ./scripts/cleanup-old-logs.sql --schema ./prisma/schema.prisma",
    "logs:backup": "npx prisma db execute --file ./scripts/backup-logs.sql --schema ./prisma/schema.prisma",
    "logs:setup-rls": "npx prisma db execute --file ./scripts/setup-unified-rls.sql --schema ./prisma/schema.prisma",
    "logs:auto-cleanup": "curl -X POST \"$NEXT_PUBLIC_APP_URL/api/cron/log-cleanup\" -H \"Authorization: Bearer $CRON_SECRET\"",
    "logs:stats": "curl -X GET \"$NEXT_PUBLIC_APP_URL/api/cron/log-cleanup\" -H \"Authorization: Bearer $CRON_SECRET\"",
    "rls:test": "npx tsx scripts/test-rls-comprehensive.ts",
    "rls:validate": "npx tsx -e \"import { validateRLSPolicies } from './lib/rls-utils.js'; validateRLSPolicies().then(console.log).catch(console.error)\"",
    "rls:benchmark": "npx tsx -e \"import { benchmarkRLS } from './lib/rls-manager.js'; benchmarkRLS().then(console.log).catch(console.error)\"",
    "docs:update": "echo 'Documentation updated. Please review the following files:' && echo '- .copilot-instructions.md' && echo '- README.md' && echo '- docs/DEPENDENCIES.md' && echo '- docs/PROJECT_STRUCTURE.md'"
  },
